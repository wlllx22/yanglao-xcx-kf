<!--components/sub-button/index.wxml-->
<block>

	<login-window closable="{{true}}" bind:success="btnHandle" class="loginwindow"></login-window>
	<van-button plain custom-style="{{custom_style}}" round="{{is_round}}" color="#fff" loading="{{loading}}" bind:click="btnHandle" icon="{{ show_icon ? 'bell' : ''}}" custom-class="btn-class">{{ sub_status ? unsub_text : sub_text}}</van-button>
</block>
